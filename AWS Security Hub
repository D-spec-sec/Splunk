index=aws "event.findings{}.ProductFields.aws/securityhub/ProductName"="Security Hub" 
| fillnull value=NULL event.findings{}.AwsAccountId event.findings{}.Severity.Label event.findings{}.FirstObservedAt event.findings{}.LastObservedAt event.findings{}.Resources{}.Type event.findings{}.Resources{}.Id event.findings{}.WorkflowState event.findings{}.Region event.findings{}.Resources{}.Details.AwsEc2Instance.SubnetId event.findings{}.Resources{}.Details.AwsEc2Instance.VpcId "event.findings{}.ProductFields.aws/inspector/resources/1/resourceDetails/awsEcrContainerImageDetails/platform" "event.findings{}.Resources{}.Details.AwsEcrContainerImage.RepositoryName" event.findings{}.Types{} event.findings{}.Vulnerabilities{}.Id 
| rename event.findings{}.* AS * Resources{}.Details.* AS * AwsEc2Instance.* AS * Severity.Label AS Severity Resources{}.Type AS ResourceType Resources{}.Id AS ResourceId "ProductFields.aws/inspector/resources/1/resourceDetails/awsEcrContainerImageDetails/platform" AS Platform AwsEcrContainerImage.RepositoryName AS RepositoryName Types{} AS VulnerabilityType Vulnerabilities{}.Id AS VulnerabilitiesId 
| lookup awsaccts.csv AWS_Acct_Number AS AwsAccountId OUTPUT AWS_Acct_Name \\ Build lookup of your AWS accounts to lookup against and table 
| table _time AwsAccountId,AWS_Acct_Name,Workflow.Status ,FindingProviderFields.Types{},LastObservedAt,Description,ResourceType,Title,RecordState,Resources{}.Tags.tu:owner-team,Resources{}.Tags.tu:bucket-name,Resources{}.Tags.aws:cloudformation:stack-name
